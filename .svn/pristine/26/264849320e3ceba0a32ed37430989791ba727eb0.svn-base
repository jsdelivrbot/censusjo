<div class="panel-body" >
  <div class="form-group">
     <label for="username" class="col-md-3 control-label">User Name</label>
     <div class="col-md-9">
         <input type="text" class="form-control" ng-model="username" name="username" placeholder="User Name" />
     </div>
  </div>
  <div class="form-group">
    <label for="password" class="col-md-3 control-label">Password</label>
    <div class="col-md-9">
      <input type="text" class="form-control" name="password" ng-model="password" placeholder="Password" />
  </div>
  <!-- Select Basic -->
  <div class="control-group">
    <label class="control-label" for="selectbasic">Role</label>
    <div class="controls">
      <select id="selectbasic" name="selectbasic" class="input-xlarge required">
        <option>LI Company Imported 1</option>
        <option>LI Company Imported 2</option>
      </select>
    </div>
  </div>
  <div class="form-group">
      <!-- Button -->
      <div class="col-md-offset-3 col-md-9">
          <button id="btn-signup" type="button" class="btn btn-primary"><i class="icon-hand-right"></i> &nbsp Sign Up</button>
      </div>
  </div>
</div>
